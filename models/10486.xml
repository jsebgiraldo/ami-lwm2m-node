<?xml version="1.0" encoding="UTF-8"?>
<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="http://www.openmobilealliance.org/tech/profiles/LWM2M-v1_1.xsd">
  <Object ObjectType="MODefinition">
    <Name>Thread CLI Command</Name>
    <Description1>This LWM2M object allows executing Thread CLI commands remotely</Description1>
    <ObjectID>10486</ObjectID>
    <ObjectURN>urn:oma:lwm2m:x:10486</ObjectURN>
    <LWM2MVersion>1.1</LWM2MVersion>
    <ObjectVersion>1.0</ObjectVersion>
    <MultipleInstances>Single</MultipleInstances>
    <Mandatory>Optional</Mandatory>
    <Resources>
      <Item ID="0">
        <Name>CLI Version</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>String</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>The version string of the OpenThread CLI implementation.</Description>
      </Item>
      <Item ID="1">
        <Name>Command</Name>
        <Operations>RW</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>String</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>The CLI command to be executed (e.g. 'state', 'rloc16', 'ipaddr').</Description>
      </Item>
      <Item ID="2">
        <Name>Execute</Name>
        <Operations>E</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type/>
        <RangeEnumeration/>
        <Units/>
        <Description>Execute the command currently set in the Command resource.</Description>
      </Item>
      <Item ID="3">
        <Name>Result</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>String</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>The result/output of the last executed command.</Description>
      </Item>
    </Resources>
    <Description2/>
  </Object>
</LWM2M>
