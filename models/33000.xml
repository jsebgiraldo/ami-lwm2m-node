<?xml version="1.0" encoding="UTF-8"?>
<LWM2M xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="http://www.openmobilealliance.org/tech/profiles/LWM2M-v1_1.xsd">
  <Object ObjectType="MODefinition">
    <Name>Thread MAC Diagnostics</Name>
    <Description1>Thread MAC-layer counters and device state for AMI nodes. Role, Partition ID, and 8 MAC counters. Network config moved to Object 10483, neighbor info to Object 10485.</Description1>
    <ObjectID>33000</ObjectID>
    <ObjectURN>urn:oma:lwm2m:x:33000:2.0</ObjectURN>
    <LWM2MVersion>1.1</LWM2MVersion>
    <ObjectVersion>2.0</ObjectVersion>
    <MultipleInstances>Single</MultipleInstances>
    <Mandatory>Optional</Mandatory>
    <Resources>
      <Item ID="0">
        <Name>Thread Role</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>String</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>Current Thread device role: Disabled, Detached, Child, Router, or Leader.</Description>
      </Item>
      <Item ID="1">
        <Name>Partition ID</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>Thread network partition identifier.</Description>
      </Item>
      <Item ID="2">
        <Name>TX Total</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>Total number of MAC frames transmitted.</Description>
      </Item>
      <Item ID="3">
        <Name>RX Total</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>Total number of MAC frames received.</Description>
      </Item>
      <Item ID="4">
        <Name>TX Unicast</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>Number of unicast MAC frames transmitted.</Description>
      </Item>
      <Item ID="5">
        <Name>RX Unicast</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>Number of unicast MAC frames received.</Description>
      </Item>
      <Item ID="6">
        <Name>TX Broadcast</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>Number of broadcast MAC frames transmitted.</Description>
      </Item>
      <Item ID="7">
        <Name>RX Broadcast</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>Number of broadcast MAC frames received.</Description>
      </Item>
      <Item ID="8">
        <Name>TX Error Abort</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>Number of MAC transmissions aborted due to errors.</Description>
      </Item>
      <Item ID="9">
        <Name>RX Error No Frame</Name>
        <Operations>R</Operations>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Mandatory</Mandatory>
        <Type>Integer</Type>
        <RangeEnumeration/>
        <Units/>
        <Description>Number of RX errors with no frame received.</Description>
      </Item>
    </Resources>
    <Description2/>
  </Object>
</LWM2M>
